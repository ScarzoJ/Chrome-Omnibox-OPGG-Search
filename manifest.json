{
  "name": "OminboxOPGGSearch",
  "description": "Simple OPGG Searchbar",
  "version": "0.1",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "omnibox": {
    "keyword": "opgg"
  },
  "options_page": "options.html"
}